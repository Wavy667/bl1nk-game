blink = false;

if (tick >= 0) {
	tick += 1/60;
}

switch (tick) {
	case 0.5:
		blink = true;
		break;
	case 1:
		surface_free(surf_blink);
		surf_blink = -1;
		tick = -1;
		break;
	default:
		blink = false;
		break;
}




















































































































































































































































































































































































