var _tick = sin(pi*tick);
if (tick > 0) {
	
	surf_blink = surface_create(room_width, room_height)
	
	surface_set_target(surf_blink);
	draw_rectangle_colour(0, 0, room_width, room_height, c_white, c_white, c_white, c_white, false);
	draw_ellipse_color(-64-(128*(1-_tick)), ((room_height/2)+64)*_tick-64, room_width+64+(128*(1-_tick)), room_height-((room_height/2)+64)*_tick+64, c_black, c_black, false);
	surface_reset_target();

	gpu_set_blendmode(bm_subtract);
	draw_surface(surf_blink, 0, 0);
	gpu_set_blendmode(bm_normal);
}

surface_free(surf_blink);















































































































































































































































































































































































